#:import layout_examples layout_examples
#:import widget_examples widget_examples
#:import navigation_screen_manager navigation_screen_manager
# #:import CardTransition kivy.uix.screenmanager.CardTransition
# https://kivy.org/doc/stable/api-kivy.uix.screenmanager.html?highlight=screenmanager#module-kivy.uix.screenmanager

MyScreenMananger:
    #transition: CardTransition()

<MyScreenMananger@NavigationScreenManager>:
    Screen:
        name: "screen1"
        BoxLayout:
            Button:
                text:'go to screen images'
                on_press: root.push("ImagesExample")
            Button:
                text:'go to screen 3'
                on_press: root.push("screen3")
    Screen:
        name: "ImagesExample"
        BoxLayout:
            orientation: 'vertical'
            ActionBar:
                ActionView:
                    ActionPrevious:
                        title: "back"
                        on_release: root.pop()
                        app_icon: "images/lab_logo.png"
                        app_icon_height: "28dp"
                        app_icon_width: "28dp"
            ImagesExample:
        
    Screen:
        name: "screen2"
        Button:
            text:'Button2'
            on_press:root.pop()
    Screen:
        name: "screen3"
        Button:
            text:'Button3'
            on_press: root.pop()
            



